    <validationRules>
        <fullName>rule1</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED( Status__c ),

ISPICKVAL(PRIORVALUE( Status__c ),&quot;Delivered&quot;)

)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>The status of order can’t be changed to any other status once it set
to be delivered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rule2</fullName>
        <active>true</active>
        <errorConditionFormula>Or(NOT( ISNUMBER( Phone__c )), 
 NOT(REGEX( Phone__c , &apos;[0-9]{10}&apos;)))</errorConditionFormula>
        <errorDisplayField>Phone__c</errorDisplayField>
        <errorMessage>the phone field must contain number and that too of 10 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rule3</fullName>
        <active>true</active>
        <errorConditionFormula>Order_Date__c &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>Order_Date__c</errorDisplayField>
        <errorMessage>The order date can’t be prior to current date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rule4</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Total_Price__c &gt;2000, ISBLANK( GST_Number__c ))</errorConditionFormula>
        <errorDisplayField>GST_Number__c</errorDisplayField>
        <errorMessage>total price exceeding Rs. 2000,
populate the GST number</errorMessage>
    </validationRules>
